          <argument>
            <kind>1</kind>
            <string>/// Bullet-to-Bullet crossing - Bigger Bullet Wins

if(id&lt;other.id &amp;&amp; speed&gt;0 &amp;&amp; team=other.team)
{
    if(abs(hspeed)&gt;abs(vspeed))
    {
        if(hspeed&gt;0)
        {
            x=other.bbox_left+(bbox_right-bbox_left);
        } else x=other.bbox_right+(bbox_left-bbox_right);
    }
    else
    {
        if(vspeed&gt;0)
        {
            y=other.bbox_bottom+(bbox_top-bbox_bottom);
        } else y=other.bbox_top+(bbox_bottom-bbox_top);
    }
}
if(team!=other.team)
{
    if(abs(attack-other.attack)&lt;5)
    {
        alarm[0]=1;
        other.alarm[0]=1;
        sound_play(snd_hit2);
    }
    else if(attack&lt;other.attack)
    {
        alarm[0] = 1;
    }
}
</string>
          </argument>
        </arguments>
      </action>
