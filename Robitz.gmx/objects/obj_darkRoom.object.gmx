            <string>/// Draw Darkness Sheet with holes at light sources

if (surface_exists(surf)) {
    surface_set_target(surf);

    draw_set_color(c_black);
    draw_set_alpha(0.8);
    draw_rectangle(0, 0, room_width, room_height, 0);

