    draw_set_color(c_white);
    
    /// ------------------ Light Sources ----------------------
    with (obj_body)
        draw_circle(x + random_range(-1, 1), y + random_range(-1, 1), other.darkness*brightness + random_range(-1, 1), false);
        
    with (obj_basicBullet)
        draw_circle(x + 2*hspeed + random_range(-1, 1), y + 2*vspeed + random_range(-1, 1), other.darkness*60 + (2*image_xscale) + random_range(-1, 1), false);
    
