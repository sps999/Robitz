            <string>/// Grapple Movement
if(attack &gt; 0)
    image_angle = 270;

if(attached.hp&lt;=0 )
{
    instance_destroy();
}

// Grapple Max Distance Reel
attached.energy = 100 - 0.5 * distance_to_point(attached.x,attached.y);
if (attached.energy &gt; 99 &amp;&amp; attack &gt; 0)
    attached.energy = 99;

if(attached.energy &lt; 0)
    attached.energy = 0;

// Reel in on Mouse Release or Distance Maxed
if(attached.attached.player=1)
{
    if (attached.energy &lt;= 0 || keyboard_check_released(vk_space))
    {
        attack = 0;
        alarm[0] = 1;
    }
}
