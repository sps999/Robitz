            <string>/// Latch to Block
block = collision_line(x,y,x,y+speed,obj_wall,false,true);

if(block != noone)
{
    if(attack &gt; 0)
    {
        speed = 0;
        y = block.bbox_top;
        
        direction = point_direction(attached.x,attached.y,x,y);
        
        attached.attached.vlaunchSpeed-=4*(attached.energy / 140);
        attached.attached.hlaunchSpeed-=0.025*(distance_to_object(attached.attached))*-cos(degtorad(direction));
        attached.attached.vlaunchSpeed*=(0.85);
        attached.attached.hlaunchSpeed*=(0.95);
        
        if(block = obj_blockBreakable)
        {
            if(block.hp &gt; 0)
            {
                block.hp-=0.1*attack;
                block.image_index=ceil(0.03*(100-block.hp));
            }
            else 
            {
                with(block) alarm[0] = 1;
                sound_play(snd_break);
                
                attack = 0;
                alarm[0] = 1;
            }
        }
    }
}
</string>
