            <string>/// Leg Properties
hp=100;
temperature=21; //Base Temperature
electricity=0;
frost=1;
image_speed=0;
image_index=0;

    /* Choice[attached.player-1] of RLeg effects properties and material
    
    Materials:
    
    Element - #     (Hardness, Melt, Heat, Density, Conductivity):
    
    Magnesium - 12  (260, 650, 160, 1738, 23)
    Aluminium - 13  (245, 660, 235, 2700, 38)
    Titanium - 22   (716, 1668, 22, 4507, 2.5)
    Chromium - 24   (1120, 1907, 94, 7140, 7.9)
    Iron - 26       (490, 1538, 80, 7874, 10)
    Nickel - 28     (700, 1455, 91, 8908, 14)
    Copper - 29     (874, 1085, 400, 8920, 59)
    Zinc - 30       (412, 420, 120, 7140, 17)
    
    Material Properties are listed in scr_material 
    
    script_execute(scr_material,material); */

// Basic Legs: Standard Jump and Speed
if(legChoice[attached.player-1]=1)
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,1);
    volume=0.190; //Temp
    movespeed=5;
    baseJump=12;
    hover=0;
}

// Jet Boots: Flame Hover
else if(legChoice[attached.player-1]=2)
{
    sprite_index=spr_rLeg02;
    script_execute(scr_material,2);
    volume=0.214; //Temp
    movespeed=6;
    baseJump=9;
    hover=1;
    baseHover=1;
    energyCost=5;
}

// Turtle Leg: Double Jump Ground Pound
else if(legChoice[attached.player-1]=3)
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,3);
    volume=0.240; //Temp
    movespeed=4;
    baseJump=16;
    hover=1;
    baseHover=0;
    energyCost=100;
}

// Biker Wheel: Fast Speed Low Jump
else if(legChoice[attached.player-1]=4)
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,4);
    volume=0.2; //Temp
    movespeed=12;
    baseJump=1;
    hover=0;
}

// Electric Leg: Extendo Legs
else if(legChoice[attached.player-1]=5)
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,5);
    volume=0.240; //Temp
    movespeed=6;
    baseJump=10;
    hover=1;
    baseHover=10;
    energyCost=0;
}

// Magnet Leg: Double Jump Slow Fall
else if(legChoice[attached.player-1]=6)
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,6);
    volume=0.240; //Temp
    movespeed=8;
    baseJump=12;
    hover=1;
    baseHover=0;
    energyCost=1;
}

else // Standard Null Legs
{
    sprite_index=spr_rLeg01;
    script_execute(scr_material,1);
    volume=0.190; //Temp
    movespeed=5;
    baseJump=12;
    hover=0;
}

if(hover=1)
{
    hovering=0;
    energyMax=100;
    energy=energyMax;
}

mass=(density*volume);
image_blend = c_material;

</string>
