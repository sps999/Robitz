          <argument>
            <kind>1</kind>
            <string>/// Damage Handling

attached.myBackLeg.image_blend=merge_color(c_material,c_red,(temperature/meltingPoint));
if(electricity&gt;0)
{
    electricity-=1;
    if(electricity&lt;0)
    {
        electricity=0;
    }
}

if(temperature&gt;meltingPoint)
{
    attached.myBackLeg.image_blend=c_red;
}

// Wheel mode after Legs Destroyed
if(hp&lt;=0 &amp;&amp; sprite_index!=spr_wheel &amp;&amp; sprite_index!=noone)
{
    repeat(2)
    {
        i=instance_create(x,y,obj_debris);
        i.sprite_index=sprite_index;
        i.direction=attached.direction+random_range(-30,30);
        i.speed=attached.speed+random(10);
        i.image_blend=image_blend;
        i.image_speed=0;
    }
    
    attached.myBackLeg.sprite_index=noone;
    sprite_index=spr_wheel;
    volume=0.1;
    movespeed=4;
    baseJump=6;
    hover=0;
    mass=100;
    image_blend=c_white;
}

if(hp&lt;=0 &amp;&amp; attached.hp&lt;=0)
{
    sprite_index=noone;
}
